{
  "name": "@bhasp/reactflow",
  "version": "11.7.4",
  "description": "A highly customizable React library for building node-based editors and interactive flow charts",
  "keywords": [
    "react",
    "node-based UI",
    "graph",
    "diagram",
    "workflow",
    "react-flow"
  ],
  "files": [
    "dist"
  ],
  "source": "src/index.ts",
  "main": "dist/umd/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "sideEffects": [
    "*.css"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wbkd/react-flow.git",
    "directory": "packages/reactflow"
  },
  "scripts": {
    "dev": "concurrently \"rollup --config node:@bhasp/rollup-config --watch\" pnpm:css-watch",
    "build": "rollup --config node:@bhasp/rollup-config --environment NODE_ENV:production && npm run css",
    "css": "postcss src/*.css --config ../../tooling/postcss-config/postcss.config.js --dir dist",
    "css-watch": "pnpm css --watch",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@bhasp/background": "workspace:*",
    "@bhasp/controls": "workspace:*",
    "@bhasp/core": "workspace:*",
    "@bhasp/minimap": "workspace:*",
    "@bhasp/node-resizer": "workspace:*",
    "@bhasp/node-toolbar": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "devDependencies": {
    "@bhasp/eslint-config": "workspace:*",
    "@bhasp/rollup-config": "workspace:*",
    "@bhasp/tsconfig": "workspace:*",
    "@types/node": "^18.7.16",
    "@types/react": ">=17",
    "react": "^18.2.0",
    "typescript": "^4.9.4"
  },
  "rollup": {
    "globals": {
      "@bhasp/background": "ReactFlowBackground",
      "@bhasp/controls": "ReactFlowControls",
      "@bhasp/core": "ReactFlowCore",
      "@bhasp/minimap": "ReactFlowMinimap",
      "@bhasp/node-toolbar": "ReactFlowNodeToolbar"
    },
    "name": "ReactFlow"
  }
}
